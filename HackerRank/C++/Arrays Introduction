#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
   int N;
    cin>>N;
    int num[N];
    for(int i=0;i<N;i++)
        {
        cin>>num[i];
    }
    for(int i=0;i<N;i++)
        {
        cout<<num[N-1-i]<<" ";
    }
       
}
